 <%= simple_form_for [@category,@product] do |f| %>
  <%= f.error_notification %>

  <%= f.input :start_month %>
  <%= f.input :start_year, collection: ["Men", "Women", "Accessory"] %>
  <%= f.input :prod_type, collection: ["T-shirt", "Jacket", "Trouser", "Shorts", "Socks", "Cap", "Longshoreman", "Oversleeve", "Scarf"] %>
  <%= f.input :price_cents %>
  <%= f.input :description %>
  <%= f.input :season %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.button :submit %>
<% end %>

