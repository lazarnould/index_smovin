 <%= simple_form_for [@lease] do |f| %>
  <%= f.error_notification %>

  <%= f.input :rent %>
  <%= f.input :start_month %>
  <%= f.input :start_year, collection: [] %>
  <%= f.input :signature_month, collection: [] %>
  <%= f.input :signature_year %>
  <%= f.input :indexation_year %>
  <%= f.input :region, collection: ["Bruxelles-Capitale", "Région Wallonne", "Région Flamande"] %>


  <%= f.button :submit %>
<% end %>
